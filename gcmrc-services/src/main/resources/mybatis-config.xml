<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE configuration
  PUBLIC '-//mybatis.org//DTD Config 3.0//EN'
  'http://mybatis.org/dtd/mybatis-3-config.dtd'>
<configuration>
	<settings>
		<setting name="logImpl" value="SLF4J"/>
	</settings>
    <typeAliases>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.AncillaryData' alias='AncillaryData'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationSite' alias='StationSite'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationQW' alias='StationQW'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationPubs' alias='StationPubs'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationCredits' alias='StationCredits'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationParam' alias='StationParam'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationBs' alias='StationBs'></typeAlias>
    	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.StationDischargeError' alias='StationDischargeError'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.Reach' alias='Reach'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.ReachDetail' alias='ReachDetail'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.ReachTrib' alias='ReachTrib'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.ReachPOR' alias='ReachPOR'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.DurationCurvePoint' alias='DurationCurvePoint'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.BedSedimentDownload' alias='BedSedimentDownload'></typeAlias>
        <typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.DurationCurveConsecutiveGap' alias='DurationCurveConsecutiveGap'></typeAlias>
	<typeAlias type='gov.usgs.cida.gcmrcservices.mb.model.DurationCurveCumulativeGap' alias='DurationCurveCumulativeGap'></typeAlias>
    </typeAliases>
    <environments default='container'>
        <environment id='container'>
            <transactionManager type="JDBC"/>
            <dataSource type="JNDI">
                <property name="data_source" value="java:comp/env/jdbc/gcmrcDS"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
    	<mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/LookupMapper.xml' />
    	<mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/StationMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/ReachMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/ReachPORMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/ReachDetailMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/ReachTribMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/DurationCurveMapper.xml' />
        <mapper resource='gov/usgs/cida/gcmrcservices/mb/mappers/BedSedimentDownloadMapper.xml' />
    </mappers>
</configuration>